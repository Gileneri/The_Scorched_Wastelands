    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_noside",
        "y": 270
      }
    }
  ]
