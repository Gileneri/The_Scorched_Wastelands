{
  "multipart": [
    {
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "the_scorched_wastelands:block/dustyglass_side",
